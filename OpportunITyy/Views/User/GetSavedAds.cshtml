@model IDictionary<Guid, String>
@{

    Layout = "~/Views/Shared/_Layout1.cshtml";
}
<h2>List of saved ads</h2>
<table class="table">

    <thead>
        <tr>

            <th style="width:20%;">Type of job</th>
            <th style="width:20%;">Ad profile</th>
            <th style="width:15%;">Remove from saved list</th>


        </tr>
    </thead>
    @foreach (var item in Model)
    {

        <tr>
            <td>@item.Value</td>
            <td>@Html.ActionLink("Ad profile", "AdProfile", new { idAd = item.Key, typeofJob = item.Value}, new { @class = "btn btn-primary" })</td>
            <td>@Html.ActionLink("Remove ad", "RemoveSavedAd", new { idAd = item.Key, typeofJob = item.Value }, new { @class = "btn btn-primary" }) </td>
        </tr>

    }
</table>